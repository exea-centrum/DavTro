<TfrxReport Version="4.6.5" DotMatrixReport="false" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="false" PreviewOptions.Buttons="4089" PreviewOptions.Zoom="1" PrintOptions.Printer="Par défaut" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="39474,8701273148" ReportOptions.Description.Text="" ReportOptions.Name="WPR DEVDET V21" ReportOptions.LastChange="39785,1006561574" ScriptLanguage="PascalScript" ScriptText.Text="// -----&#13;&#10;// Variables générales                                                            &#13;&#10;// -----                 &#13;&#10;var&#13;&#10;unitpix : extended;&#13;&#10;valpouce : extended;&#13;&#10;fontt : string;&#13;&#10;fonts : integer;&#13;&#10;quincfix : boolean;                                          &#13;&#10;sitmac : string;&#13;&#10;txtmac : string;&#13;&#10;messchass : string;                                         &#13;&#10;sstotvol : extended;                                               &#13;&#10;sstotvit : extended;                                               &#13;&#10;sstotchas : extended;                                               &#13;&#10;sstotacc : extended;    &#13;&#10;fournprof : extended;    &#13;&#10;fourntole : extended;    &#13;&#10;fournart : extended;    &#13;&#10;fournpose : extended;    &#13;&#10;prtot : extended;&#13;&#10;remiglob : boolean;&#13;&#10;remiglobfourn : boolean;&#13;&#10;remivol : boolean;&#13;&#10;remichass : boolean;&#13;&#10;remivitr : boolean;&#13;&#10;remicde : boolean;                                        &#13;&#10;valeurvariante : boolean;                                        &#13;&#10;prixvarprinc : extended;                                                          &#13;&#10;cptouvrant : integer;&#13;&#10;cptcroisillon : integer;&#13;&#10;cptaerateur : integer;&#13;&#10;cptrehausse : integer;&#13;&#10;cptcoupleur : integer;&#13;&#10;cpttraverseD : integer;&#13;&#10;cpttraverseO : integer;&#13;&#10;cptfxcintre : integer;&#13;&#10;cpttablier : integer;&#13;&#10;languedoc : string;&#13;&#10;titreclientdoc : string;&#13;&#10;nomclientdoc : string;&#13;&#10;adresseclientdoc : string;&#13;&#10;zipclientdoc : string;&#13;&#10;localiteclientdoc : string;&#13;&#10;paysclientdoc : string;&#13;&#10;coorcliprosdoc1 : string;                                                  &#13;&#10;coorcliprosdoc2 : string;                                                  &#13;&#10;coorcliprosdoc3 : string;                                                  &#13;&#10;coorcliprosdoc4 : string;                                                  &#13;&#10;dessinchassisEMF : boolean;                                                        &#13;&#10;dessinarticleEMF : boolean;                                                        &#13;&#10;dessinfournitureEMF : boolean;                                                        &#13;&#10;      &#13;&#10;procedure MainOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   exporttable : integer;&#13;&#10;   exporttablecomplet : integer;&#13;&#10;   exportEMF : integer;&#13;&#10;   exportEMFcomplet : integer;&#13;&#10;&#13;&#10;begin&#13;&#10;// Message d'erreur si pas d'export ou pas de dessin EMF                                                                                                                                                      &#13;&#10;   exporttable := ReadSettingInteger('Mode Sortie Documents','Devis Detaille',0);                                      &#13;&#10;   exporttablecomplet := ReadSettingInteger('Mode Sortie Documents','Complet',0);                                      &#13;&#10;   exportEMF := ReadSettingInteger('Mode Sortie Documents','Dessin',0);                                      &#13;&#10;   exportEMFcomplet := ReadSettingInteger('Mode Sortie Documents','Dessin Complet',0);                                      &#13;&#10;// Message si pas d'export                                                        &#13;&#10;   if ((exporttable &#60; 2) and (exporttablecomplet = 0)) then&#13;&#10;      begin&#13;&#10;         messagedlg('Vous n''avez pas défini d''export pour le Devis détaillé ! Le document ne sera pas imprimé... ', mtconfirmation, mbok, 0);  &#13;&#10;         engine.stopreport;                                                                                     &#13;&#10;      end;&#13;&#10;// Message si pas d'EMF                                                                &#13;&#10;   if ((exportEMF &#60; 1) and (exporttablecomplet &#60;&#62; 1) and (exporttablecomplet &#60;&#62; 3)) then&#13;&#10;      begin&#13;&#10;         messagedlg('Vous n''avez pas défini d''export de dessin EMF de châssis pour le rapport Devis détaillé ! Le document ne sera pas imprimé... ', mtconfirmation, mbok, 0);  &#13;&#10;         engine.stopreport;                                                                                     &#13;&#10;      end;  &#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   codeclt : integer;                                                    &#13;&#10;       &#13;&#10;begin&#13;&#10;// Initialisation des paramètres liés au client ou au prospect&#13;&#10;   codeclt := ReadSettingInteger('Documents','NumClient',0);&#13;&#10;   if (&#60;commande.&#34;typecli&#34;&#62; = 1) then&#13;&#10;      begin&#13;&#10;         languedoc := &#60;prospect.&#34;langue&#34;&#62;;                                                                                          &#13;&#10;         titreclientdoc := &#60;prospect.&#34;titre&#34;&#62;;&#13;&#10;         nomclientdoc := &#60;prospect.&#34;nom&#34;&#62;;&#13;&#10;         adresseclientdoc := &#60;prospect.&#34;adresse&#34;&#62;;&#13;&#10;         zipclientdoc := &#60;prospect.&#34;zip&#34;&#62;;&#13;&#10;         localiteclientdoc := &#60;prospect.&#34;localite&#34;&#62;;&#13;&#10;         paysclientdoc := &#60;prospect.&#34;pays&#34;&#62;;&#13;&#10;         if not (&#60;prospect.&#34;numtva&#34;&#62; = '') then&#13;&#10;            coorcliprosdoc1 := Translate('DOC_TVA',languedoc) + ': ' + Trim(&#60;prospect.&#34;numtva&#34;&#62;);                                                                                        &#13;&#10;         if not ((&#60;prospect.&#34;tel1&#34;&#62; = '') and (&#60;prospect.&#34;tel2&#34;&#62; = '')) then&#13;&#10;            coorcliprosdoc2 := Translate('DOC_TEL',languedoc) + ': ' + Trim(&#60;prospect.&#34;tel1&#34;&#62;) + '; ' + Trim(&#60;prospect.&#34;tel2&#34;&#62;);                                                                                          &#13;&#10;         if not (&#60;prospect.&#34;fax1&#34;&#62; = '') then&#13;&#10;            coorcliprosdoc3 := Translate('DOC_FAX',languedoc) + ': ' + Trim(&#60;prospect.&#34;fax1&#34;&#62;);                                                                                               &#13;&#10;         if not (codeclt = 0) then&#13;&#10;            coorcliprosdoc4 := Translate('DOC_NOCLIENT',languedoc) + ': ' + &#60;prospect.&#34;code&#34;&#62;;                                                                                               &#13;&#10;      end&#13;&#10;   else               &#13;&#10;      begin&#13;&#10;         languedoc := &#60;client.&#34;langue&#34;&#62;;                                                                                          &#13;&#10;         titreclientdoc := &#60;client.&#34;titre&#34;&#62;;&#13;&#10;         nomclientdoc := &#60;client.&#34;nom&#34;&#62;;&#13;&#10;         adresseclientdoc := &#60;client.&#34;adresse&#34;&#62;;&#13;&#10;         zipclientdoc := &#60;client.&#34;zip&#34;&#62;;&#13;&#10;         localiteclientdoc := &#60;client.&#34;localite&#34;&#62;;&#13;&#10;         paysclientdoc := &#60;client.&#34;pays&#34;&#62;;&#13;&#10;         if not (&#60;client.&#34;numtva&#34;&#62; = '') then&#13;&#10;            coorcliprosdoc1 := Translate('DOC_TVA',languedoc) + ': ' + Trim(&#60;client.&#34;numtva&#34;&#62;);                                                                                        &#13;&#10;         if not ((&#60;client.&#34;tel1&#34;&#62; = '') and (&#60;client.&#34;tel2&#34;&#62; = '') and (&#60;client.&#34;tel3&#34;&#62; = '')) then&#13;&#10;            coorcliprosdoc2 := Translate('DOC_TEL',languedoc) + ': ' + Trim(&#60;client.&#34;tel1&#34;&#62;) + '; ' + Trim(&#60;client.&#34;tel2&#34;&#62;) + '; ' + Trim(&#60;client.&#34;tel3&#34;&#62;);                                                                                          &#13;&#10;         if not ((&#60;client.&#34;fax1&#34;&#62; = '') and (&#60;client.&#34;fax2&#34;&#62; = '') and (&#60;client.&#34;fax3&#34;&#62; = '')) then&#13;&#10;            coorcliprosdoc3 := Translate('DOC_FAX',languedoc) + ': ' + Trim(&#60;client.&#34;fax1&#34;&#62;) + '; ' + Trim(&#60;client.&#34;fax2&#34;&#62;) + '; ' + Trim(&#60;client.&#34;fax3&#34;&#62;);                                                                                               &#13;&#10;         if not (codeclt = 0) then&#13;&#10;            coorcliprosdoc4 := Translate('DOC_NOCLIENT',languedoc) + ': ' + &#60;client.&#34;code&#34;&#62;;                                                                                               &#13;&#10;      end;&#13;&#10;end;&#13;&#10;                        &#13;&#10;procedure logosocOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   logol : extended;                                      &#13;&#10;   logot : extended;                                      &#13;&#10;   logow : extended;                                      &#13;&#10;   logoh : extended;                                      &#13;&#10;      &#13;&#10;begin&#13;&#10;// Calage du logo (préférences)                                                                  &#13;&#10;   if not (&#60;societe.&#34;logo&#34;&#62; = '') then&#13;&#10;   begin&#13;&#10;      logosoc.visible := true();&#13;&#10;      logol := ReadSettingFloat('Documents','Logo left',0);                                      &#13;&#10;      logot := ReadSettingFloat('Documents','Logo top',0);                                      &#13;&#10;      logow := ReadSettingFloat('Documents','Logo width',0);                                      &#13;&#10;      logoh := ReadSettingFloat('Documents','Logo height',0);                                      &#13;&#10;      logosoc.left := logol * unitpix * valpouce;&#13;&#10;      logosoc.top := logot * unitpix * valpouce;&#13;&#10;      logosoc.width := logow * unitpix * valpouce;&#13;&#10;      logosoc.height := logoh * unitpix * valpouce;&#13;&#10;      logosoc.loadfromfile(&#60;societe.&#34;logo&#34;&#62;);&#13;&#10;   end&#13;&#10;   else&#13;&#10;      logosoc.visible := false();       &#13;&#10;end;  &#13;&#10;&#13;&#10;   &#13;&#10;procedure societenomOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   socl : extended;                                                    &#13;&#10;   soct : extended;                                                    &#13;&#10;   socw : extended;                                                    &#13;&#10;   soch : extended;                                                    &#13;&#10;      &#13;&#10;begin&#13;&#10;// Calage du nom de société (préférences)                                                                  &#13;&#10;   if (&#60;societe.&#34;logo&#34;&#62; = '') then&#13;&#10;   begin&#13;&#10;      societenom.visible := true();  &#13;&#10;      socl := ReadSettingFloat('Documents','Company left',0);                                      &#13;&#10;      soct := ReadSettingFloat('Documents','Company top',0);                                      &#13;&#10;      socw := ReadSettingFloat('Documents','Company width',0);                                      &#13;&#10;      soch := ReadSettingFloat('Documents','Company height',0);                                      &#13;&#10;      societenom.left := &#60;settings.&#34;logol&#34;&#62; * unitpix * valpouce;&#13;&#10;      societenom.top := &#60;settings.&#34;logot&#34;&#62; * unitpix * valpouce;&#13;&#10;      societenom.width := &#60;settings.&#34;logow&#34;&#62; * unitpix * valpouce;&#13;&#10;      societenom.height := &#60;settings.&#34;logoh&#34;&#62; * unitpix * valpouce;&#13;&#10;      societenom.font.size := fonts + 4;&#13;&#10;      societenom.font.style := fsbold;&#13;&#10;      &#13;&#10;      societesoustitre.visible := true();  &#13;&#10;      societesoustitre.top := societenom.top + societenom.height;&#13;&#10;      societesoustitre.left := societenom.left;&#13;&#10;      societesoustitre.width := societenom.width;          &#13;&#10;      societesoustitre.font.size := fonts + 2;&#13;&#10;  &#13;&#10;      societetexte1.visible := true();&#13;&#10;      societetexte1.top := societesoustitre.top + societesoustitre.height;&#13;&#10;      societetexte1.left := societenom.left;&#13;&#10;      societetexte1.width := societenom.width;          &#13;&#10;&#13;&#10;      societetexte2.visible := true();&#13;&#10;      societetexte2.top := societetexte1.top + societetexte1.height;&#13;&#10;      societetexte2.left := societenom.left;&#13;&#10;      societetexte2.width := societenom.width;    &#13;&#10;   end&#13;&#10;   else&#13;&#10;   begin&#13;&#10;      societenom.visible := false();&#13;&#10;      societesoustitre.visible := false();&#13;&#10;      societetexte1.visible := false();&#13;&#10;      societetexte2.visible := false();&#13;&#10;   end             &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure typedocOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   custl : extended;&#13;&#10;   custt : extended;                                                    &#13;&#10;   custw : extended;                                                    &#13;&#10;   custh : extended;&#13;&#10;   versdoc : integer;                                           &#13;&#10;&#13;&#10;begin&#13;&#10;// Calage du nom client (préférences)                                                                    &#13;&#10;   custl := ReadSettingFloat('Documents','Customer left',0);                                      &#13;&#10;   custt := ReadSettingFloat('Documents','Customer top',0);                                      &#13;&#10;   custw := ReadSettingFloat('Documents','Customer width',0);                                      &#13;&#10;   custh := ReadSettingFloat('Documents','Customer height',0);                                      &#13;&#10;   typedoc.left := custl * unitpix * valpouce;&#13;&#10;   typedoc.width := custw * unitpix * valpouce;&#13;&#10;//   typedoc.height := custh * unitpix * valpouce;&#13;&#10;   typedoc.top := custt * unitpix * valpouce;&#13;&#10;   typedoc.font.size := fonts + 4;&#13;&#10;  &#13;&#10;   versdoc := ReadSettingInteger('Documents','Version',0);                                      &#13;&#10;   if (versdoc &#60;&#62; 0) then&#13;&#10;      begin&#13;&#10;      versiondoc.visible := true();&#13;&#10;      versiondoc.left := custl * unitpix * valpouce;&#13;&#10;      versiondoc.width := typedoc.width;&#13;&#10;      versiondoc.top := typedoc.top + typedoc.height;&#13;&#10;      versiondoc.font.size := fonts + 4;&#13;&#10;      end&#13;&#10;   else&#13;&#10;      begin&#13;&#10;      versiondoc.visible := false();&#13;&#10;      end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure nomcliOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   nomcli.left := versiondoc.left;&#13;&#10;   nomcli.top := versiondoc.top + versiondoc.height + 5;&#13;&#10;   nomcli.width := versiondoc.width;&#13;&#10;   nomcli.font.size := fonts + 2;&#13;&#10;   nomcli.font.style := fsbold;&#13;&#10;      &#13;&#10;end;&#13;&#10;&#13;&#10;procedure coorcliOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   codeclt : integer;                                                    &#13;&#10;begin&#13;&#10;   codeclt := ReadSettingInteger('Documents','NumClient',0);                                      &#13;&#10;   coorcli.left := versiondoc.left;&#13;&#10;   coorcli.top := nomcli.top + 30;&#13;&#10;   coorcli.memo.clear;                                                                                                  &#13;&#10;   if not (coorcliprosdoc1 = '') then                                                   &#13;&#10;      coorcli.memo.add(coorcliprosdoc1);&#13;&#10;   if not (coorcliprosdoc2 = '') then                                                   &#13;&#10;      coorcli.memo.add(coorcliprosdoc2);&#13;&#10;   if not (coorcliprosdoc3 = '') then                                                   &#13;&#10;      coorcli.memo.add(coorcliprosdoc3);&#13;&#10;   if not (coorcliprosdoc4 = '') then                                                   &#13;&#10;      coorcli.memo.add(coorcliprosdoc4);&#13;&#10;                                                                                                                            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Bde_AdrLivOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if ((&#60;commande.&#34;zonelivr&#34;&#62; = '') and (&#60;cde.&#34;adrlivr&#34;&#62; = '')) then                                        &#13;&#10;      Bde_AdrLiv.visible := false&#13;&#10;   else&#13;&#10;      Bde_AdrLiv.visible := true;                 &#13;&#10;end;&#13;&#10;    &#13;&#10;procedure AdrLivrOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if not (&#60;commande.&#34;zonelivr&#34;&#62; = '') then                                        &#13;&#10;      AdrLivr.memo.add(Translate('DOC_ZONE',languedoc) + ' #' + &#60;commande.&#34;zonelivr&#34;&#62;);      &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterDataFENOnBeforePrint(Sender: TfrxComponent);&#13;&#10;// Compteurs pour ouvrants, rehausses, tabliers, faux cintres, traverses                                                                                                                                                                      &#13;&#10;begin&#13;&#10;cptouvrant := 0;&#13;&#10;cptrehausse := 0;&#13;&#10;cptcoupleur := 0;&#13;&#10;cpttraverseD := 0;&#13;&#10;cpttraverseO := 0;&#13;&#10;cptfxcintre := 0;&#13;&#10;cpttablier := 0;&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure NumChassOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if (&#60;fen.&#34;variante&#34;&#62; = 1) then&#13;&#10;      begin&#13;&#10;         numchass.memo.add(Translate('DOC_VARIANTE',languedoc)+ ' ' + Translate('DOC_CHASSIS',languedoc) + ' ' + &#60;fen.&#34;position&#34;&#62;);  &#13;&#10;         SysChass.visible := true;                                                                                                                                                                        &#13;&#10;         ParcChass.visible := true;                                                                                                                                                                        &#13;&#10;         CoulrChass.visible := true;                                                                                                                                                                        &#13;&#10;      end                &#13;&#10;   else if ((&#60;fen.&#34;ensemble&#34;&#62; = 1) and (&#60;fen.&#34;partie&#34;&#62; = '')) then                      &#13;&#10;      begin&#13;&#10;         numchass.memo.add(Translate('DOC_ASSEMBLAGE',languedoc) + ' ' + &#60;fen.&#34;position&#34;&#62;);                                                                                                               &#13;&#10;         SysChass.visible := true;                                                                                                                                                                        &#13;&#10;         ParcChass.visible := false;                                                                                                                                                                        &#13;&#10;         CoulrChass.visible := true;                                                                                                                                                                        &#13;&#10;         SysChass.width := SysChass.width + ParcChass.width;                                                                                                                                                                        &#13;&#10;      end           &#13;&#10;   else if (&#60;fen.&#34;type&#34;&#62; = 2) then                      &#13;&#10;      begin&#13;&#10;         numchass.memo.add(Translate('DOC_VOLET',languedoc) + ' ' + &#60;fen.&#34;position&#34;&#62;);                                                                                                               &#13;&#10;         SysChass.visible := false;                                                                                                                                                                        &#13;&#10;         ParcChass.visible := false;                                                                                                                                                                        &#13;&#10;         CoulrChass.visible := false;                                                                                                                                                                        &#13;&#10;         QteChass.width := (Main.paperwidth - NumChass.width / unitpix - Main.leftmargin - Main.rightmargin) * unitpix;                                                                                                                                                                        &#13;&#10;      end           &#13;&#10;   else&#13;&#10;      begin&#13;&#10;         numchass.memo.add(Translate('DOC_CHASSIS',languedoc) + ' ' + &#60;fen.&#34;position&#34;&#62;);                                                                                                               &#13;&#10;         SysChass.visible := true;                                                                                                                                                                        &#13;&#10;         ParcChass.visible := true;                                                                                                                                                                        &#13;&#10;         CoulrChass.visible := true;                                                                                                                                                                        &#13;&#10;         SysChass.width := 228;                                                                                                                                                                        &#13;&#10;         ParcChass.width := 232;                                                                                                                                                                        &#13;&#10;         QteChass.width := 64;                                                                                                                                                                        &#13;&#10;      end                                               &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (engine.freespace &#60; 40) then&#13;&#10;//     Header1.visible := false&#13;&#10;  else&#13;&#10;//     Header1.visible := true;        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure PP_NumCdeOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   versdoc : integer;                                                    &#13;&#10;begin&#13;&#10;   PP_NumCde.font.size := fonts + 4;&#13;&#10;   PP_NumCde.memo.clear;&#13;&#10;   versdoc := ReadSettingInteger('Documents','Versio