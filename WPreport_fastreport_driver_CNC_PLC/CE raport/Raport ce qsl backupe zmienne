ar := (68 * Wysokosc * 2) + (68 * Szerokosc * 2)
ur := 1.4
as := (Szerokosc - 68 - 38 + 16) * (Wysokosc - 68 - 38 + 14)
a := Szerokosc * Wysokosc
uos := 1.1
fi := 0.02
l := ((Szerokosc - 68 - 38 + 16) * 2) + ((Wysokosc - 68 - 38 + 16) * 2)

u := ((uos * as) + (ur * ar) + (fi * l)) / a
Ilosc := u / 1

