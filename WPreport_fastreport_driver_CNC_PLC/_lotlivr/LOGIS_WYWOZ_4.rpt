[REPORT=LOGIS_WYWOZ_4     1]
[C]database=stasiu
[I]line=1
[C]name=detcolis
[M]params=642
SELECT `detcolis`.`article`, sum(`detcolis`.`qte`) as 'suma',`detcolis`.`longueur`,`detcolis`.`unite`, `detcolis`.`descriptio`,`detcolis`.`clrint`,`detcolis`.`clrext`,`detcolis`.`datepack`, `detcolis`.`client`,`detcolis`.`commande`,`detcolis`.`hauteur`,`detcolis`.`largeur`, `detcolis`.`lotlivr`, `lotlivraison`.`lot`, `lotlivraison`.`date`, `lotlivraison`.`code`, `lotlivraison`.`comment`
FROM `winpro2`.`detcolis`, `winpro2`.`lotlivraison`
where
     (`detcolis`.`lotlivr`=`lotlivraison`.`lot`)
and  (`detcolis`.`lotlivr` like  :mon_param)
and   `detcolis`.`chassis`='0'
and longueur <> 0

group by  client, article
order by client
[C]report=LOGIS_WYWOZ_4
[C]type=Q
[End of REPORTQRY]
