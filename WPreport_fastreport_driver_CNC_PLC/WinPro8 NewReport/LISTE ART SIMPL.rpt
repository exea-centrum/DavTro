    Sender.Tag:=tmp&#13;&#10;       end                             &#13;&#10;       else&#13;&#10;       begin                           &#13;&#10;          Sender.Tag:=0;&#13;&#10;                                     &#13;&#10;          if (CheckBox1.Tag&#62;t)then&#13;&#10;          begin&#13;&#10;             CheckBox1.Tag:=CheckBox1.Tag-1                                                        &#13;&#10;          end;                    &#13;&#10;          if (CheckBox2.Tag&#62;t) then&#13;&#10;          begin&#13;&#10;             CheckBox2.Tag:=CheckBox2.Tag-1                  &#13;&#10;          end;&#13;&#10;          &#13;&#10;          if (CheckBox3.Tag&#62;t)then&#13;&#10;          begin&#13;&#10;             CheckBox3.Tag:=CheckBox3.Tag-1                                                                 &#13;&#10;          end;&#13;&#10;         &#13;&#10;          if (CheckBox4.Tag&#62;t) then&#13;&#10;          begin&#13;&#10;             CheckBox4.Tag:=CheckBox4.Tag-1                                                                 &#13;&#10;          end;                                                                     &#13;&#10;       end;                 &#13;&#10;  &#13;&#10;      NumerosOrdre;                                           &#13;&#10;end;&#13;&#10;&#13;&#10;//+++++++++++++++++++ Numeros d'ordre des tris +++++++++++++++                                                                                                                                 &#13;&#10;  &#13;&#10;&#13;&#10;procedure NumerosOrdre;  &#13;&#10;begin&#13;&#10;       if CheckBox1.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit2.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit2.Text:=IntToStr(CheckBox1.Tag)                                                                       &#13;&#10;       end;&#13;&#10;       &#13;&#10;       if CheckBox2.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit6.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit6.Text:=IntToStr(CheckBox2.Tag)                                                                       &#13;&#10;       end;&#13;&#10;       &#13;&#10;       if CheckBox3.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit7.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit7.Text:=IntToStr(CheckBox3.Tag)                                                                       &#13;&#10;       end;&#13;&#10;       &#13;&#10;       if CheckBox4.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit8.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit8.Text:=IntToStr(CheckBox4.Tag)                                                                       &#13;&#10;       end;           &#13;&#10;end;            &#13;&#10;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                  &#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                  &#13;&#10;    &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);                                                &#13;&#10;var &#13;&#10;   sql1 : string;&#13;&#10;   compl : string;&#13;&#10;   inv : string  ;&#13;&#10;   from : string;                                  &#13;&#10;&#13;&#10;begin&#13;&#10;   sql1 := 'SELECT a.code, a.descriptio, af.codefou, a.artfou, a.categorie, a.unite, af.numero';      &#13;&#10;     &#13;&#10;   //+++ Section Critères de sélection +++                                                                                        &#13;&#10;   if isblank(Edit1.Text) then&#13;&#10;   begin&#13;&#10;      if isblank(Edit3.Text) then&#13;&#10;      begin&#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= ''                                                                                                                                                                &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= ' a.codefou=''' + Edit4.Text + ''''          &#13;&#10;            end;&#13;&#10;         end&#13;&#10;         else&#13;&#10;         begin&#13;&#10;            compl:= ' a.categorie=''' + ComboBox1.Text + '''';  &#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;                &#13;&#10;         end;&#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         compl:= ' a.code&#60;=''' + Edit3.Text+'''';                                                                                                            &#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text +''''        &#13;&#10;            end;&#13;&#10;         end&#13;&#10;         else&#13;&#10;         begin&#13;&#10;            compl:= compl + ' and a.categorie=''' +  ComboBox1.Text+'''';  &#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;                &#13;&#10;         end;          &#13;&#10;      end;                &#13;&#10;   end&#13;&#10;   else&#13;&#10;   begin&#13;&#10;      compl := ' a.code&#62;=''' + Edit1.Text+'''';                                                                                         &#13;&#10;      if isblank(Edit3.Text) then&#13;&#10;      begin&#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;           