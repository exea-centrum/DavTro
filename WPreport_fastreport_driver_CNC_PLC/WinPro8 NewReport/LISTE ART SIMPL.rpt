           &#13;&#10;         begin&#13;&#10;            inv := ' and s.qte&#62;0'                         &#13;&#10;         end;&#13;&#10;         from := ', s.qte FROM article a join artfou af on af.article = a.code join stock s on s.article=a.code';                                             &#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         from := ' FROM article a join artfou af on af.article = a.code';&#13;&#10;      end;                           &#13;&#10;   end&#13;&#10;   else&#13;&#10;   begin&#13;&#10;      from := ' FROM article a join artfou af on af.article = a.code';               &#13;&#10;   end;             &#13;&#10;&#13;&#10;   sql1 := sql1 + from + ' where af.numero=0' + inv;&#13;&#10;&#13;&#10;     &#13;&#10;   if not (isblank(compl)) then&#13;&#10;   begin                 &#13;&#10;      sql1 := sql1 + ' and' + compl;&#13;&#10;&#13;&#10;   end;                  &#13;&#10;      &#13;&#10;   OrdreTris;&#13;&#10;&#13;&#10;   sql1 := sql1 + tris;                                                     &#13;&#10;&#13;&#10;        &#13;&#10;   ReplaceSQL('Articles',sql1);                           &#13;&#10;                           &#13;&#10;&#13;&#10;//+++++++  Messages de debogage  ++++++++                                                                                                                     &#13;&#10;//   ShowMessage('Requête SQL1: '+ sql1);&#13;&#10;&#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                                                &#13;&#10;//+++++++++++++++++++  Ordre des tris dans requête SQL  ++++++++++++++++&#13;&#10;&#13;&#10;procedure OrdreTris;&#13;&#10;var&#13;&#10;   tri : Array[1..4] of string;                            &#13;&#10;   i : integer;                               &#13;&#10;begin&#13;&#10;   if CheckBox1.