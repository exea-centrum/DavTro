<TfrxReport Version="4.2.14" DotMatrixReport="false" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="39163,3614629745" ReportOptions.Description.Text="" ReportOptions.Name="HISTO MVT STK" ReportOptions.LastChange="39163,3614629745" ScriptLanguage="PascalScript" ScriptText.Text="//++++++++++++ Déclarations variables globales  ++++++++++                                                                                                                            &#13;&#10;var&#13;&#10;som : Extended;                                  &#13;&#10;tris : string;                              &#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;    &#13;&#10;function isblank (chaine : string) : boolean;&#13;&#10;begin&#13;&#10;  if (length(trim(chaine)) &#62; 0) then             &#13;&#10;     Result := false  &#13;&#10;  else&#13;&#10;     Result := true;&#13;&#10;end;                      &#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;      &#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure JnlStkarticleOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;contenu : string;                         &#13;&#10;begin&#13;&#10;   if not (isblank(&#60;JnlStk.&#34;article&#34;&#62;)) then&#13;&#10;   begin&#13;&#10;      contenu :=&#60;JnlStk.&#34;article&#34;&#62;;&#13;&#10;      contenu := contenu + ': ';                                                                 &#13;&#10;   end;&#13;&#10;   if not (isblank(&#60;JnlStk.&#34;descriptio&#34;&#62;)) then&#13;&#10;   begin&#13;&#10;      contenu := contenu + &#60;JnlStk.&#34;descriptio&#34;&#62;;                                                                            &#13;&#10;   end;&#13;&#10;   if not (isblank(&#60;JnlStk.&#34;categorie&#34;&#62;)) then&#13;&#10;   begin&#13;&#10;      contenu := contenu + '(';&#13;&#10;      contenu := contenu + &#60;JnlStk.&#34;categorie&#34;&#62;;&#13;&#10;      contenu := contenu + ')';          &#13;&#10;   end;             &#13;&#10;   JnlStkarticle.Memo.add(contenu);       &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                    &#13;&#10;  &#13;&#10;procedure JnlStktypeOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if (&#60;JnlStk.&#34;type&#34;&#62;='V') then&#13;&#10;   begin&#13;&#10;      JnlStktype.Memo.Text := Translate('IDS_SORTIE','');                                                                              &#13;&#10;   end;             &#13;&#10;   if (&#60;JnlStk.&#34;type&#34;&#62;='A') then&#13;&#10;   begin&#13;&#10;      JnlStktype.Memo.Text := Translate('IDS_ENTREE','');                            &#13;&#10;   end;             &#13;&#10;   if (&#60;JnlStk.&#34;type&#34;&#62;='C') then&#13;&#10;   begin&#13;&#10;      JnlStktype.Memo.Text := Translate('IDS_CORRECTION','');                            &#13;&#10;   end;&#13;&#10;   if (&#60;JnlStk.&#34;type&#34;&#62;='T') then&#13;&#10;   begin&#13;&#10;      JnlStktype.Memo.Text := Translate('IDS_TRANSFERT','');                            &#13;&#10;   end;       &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                    &#13;&#10;    &#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo9.Text := FormatFloat('0.000',som);                                                                                                                                           &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                    &#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   som := 0.0;                                                                          &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   &#13;&#10;   if not CheckBox8.Checked then&#13;&#10;   begin&#13;&#10;      if &#60;JnlStk.&#34;type&#34;&#62;='A' then&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := false;                                                                                                           &#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := true;                                        &#13;&#10;      end;                &#13;&#10;   end;             &#13;&#10;   &#13;&#10;   if not CheckBox9.Checked then&#13;&#10;   begin&#13;&#10;      if &#60;JnlStk.&#34;type&#34;&#62;='V' then&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := false;                                                                                                           &#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := true;                                        &#13;&#10;      end;       &#13;&#10;   end;             &#13;&#10;   &#13;&#10;   if not CheckBox10.Checked then&#13;&#10;   begin&#13;&#10;      if &#60;JnlStk.&#34;type&#34;&#62;='C' then&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := false;                                                                                                           &#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := true;                                        &#13;&#10;      end;       &#13;&#10;   end;             &#13;&#10;   &#13;&#10;   if not CheckBox11.Checked then&#13;&#10;   begin&#13;&#10;      if &#60;JnlStk.&#34;type&#34;&#62;='T' then&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := false;                                                                                                           &#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         MasterData1.Visible := true;                                        &#13;&#10;      end;       &#13;&#10;   end;             &#13;&#10;&#13;&#10;   if MasterData1.Visible then&#13;&#10;   begin&#13;&#10;      som := som + &#60;JnlStk.&#34;qte&#34;&#62;;&#13;&#10;   end;             &#13;&#10;     &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;&#13;&#10;procedure CheckBox1OnClick(Sender: TfrxCheckBoxControl);&#13;&#10;var&#13;&#10;   tmp, t : integer;                                                          &#13;&#10;begin&#13;&#10;    tmp:=0;&#13;&#10;     t:=Sender.Tag;                                                             &#13;&#10;         &#13;&#10;       if CheckBox1.Checked=true then&#13;&#10;       begin                &#13;&#10;          tmp:=tmp+1   &#13;&#10;       end;&#13;&#10;      &#13;&#10;       if CheckBox2.Checked then&#13;&#10;       begin                &#13;&#10;          tmp:=tmp+1  &#13;&#10;       end;&#13;&#10;      &#13;&#10;       if CheckBox3.Checked then&#13;&#10;       begin                &#13;&#10;          tmp:=tmp+1&#13;&#10;       end;                     &#13;&#10;    &#13;&#10;             &#13;&#10;&#13;&#10;       if Sender.Checked then&#13;&#10;       begin                   &#13;&#10;          Sender.Tag:=tmp&#13;&#10;       end                             &#13;&#10;       else&#13;&#10;       begin                           &#13;&#10;          Sender.Tag:=0;&#13;&#10;                                     &#13;&#10;          if (CheckBox1.Tag&#62;t)then&#13;&#10;          begin&#13;&#10;             CheckBox1.Tag:=CheckBox1.Tag-1                                                        &#13;&#10;          end;                    &#13;&#10;          if (CheckBox2.Tag&#62;t) then&#13;&#10;          begin&#13;&#10;             CheckBox2.Tag:=CheckBox2.Tag-1                  &#13;&#10;          end;&#13;&#10;          &#13;&#10;          if (CheckBox3.Tag&#62;t)then&#13;&#10;          begin&#13;&#10;             CheckBox3.Tag:=CheckBox3.Tag-1                                                                 &#13;&#10;          end;&#13;&#10;         &#13;&#10;       end;                 &#13;&#10;  &#13;&#10;      NumerosOrdre;                                           &#13;&#10;end;&#13;&#10;&#13;&#10;//+++++++++++++++++++ Numeros d'ordre des tris +++++++++++++++                                                                                                                                 &#13;&#10;  &#13;&#10;&#13;&#10;procedure NumerosOrdre;  &#13;&#10;begin&#13;&#10;       if CheckBox1.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit6.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit6.Text:=IntToStr(CheckBox1.Tag)                                                                       &#13;&#10;       end;&#13;&#10;       &#13;&#10;       if CheckBox2.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit7.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit7.Text:=IntToStr(CheckBox2.Tag)                                                                       &#13;&#10;       end;&#13;&#10;       &#13;&#10;       if CheckBox3.Tag=0 then&#13;&#10;       begin&#13;&#10;          Edit8.Text:=''                                                                                                  &#13;&#10;       end&#13;&#10;       else&#13;&#10;       begin&#13;&#10;          Edit8.Text:=IntToStr(CheckBox3.Tag)                                                                       &#13;&#10;       end;&#13;&#10;           &#13;&#10;end;&#13;&#10;&#13;&#10;//+++++++++++++++++++  Ordre des tris dans requête SQL  ++++++++++++++++&#13;&#10;&#13;&#10;procedure OrdreTris;&#13;&#10;var&#13;&#10;   tri : array[1..3] of string;&#13;&#10;   i : integer;                               &#13;&#10;begin&#13;&#10;    &#13;&#10;   if CheckBox1.Tag&#62;0 then     &#13;&#10;      tri[CheckBox1.Tag] := 'j.article';&#13;&#10;   if CheckBox2.Tag&#62;0 then       &#13;&#10;      tri[CheckBox2.Tag] := 'a.categorie';&#13;&#10;   if CheckBox3.Tag&#62;0 then       &#13;&#10;      tri[CheckBox3.Tag] := 'j.fourn';     &#13;&#10;&#13;&#10;   if length(trim(tri[1]))&#62;0 then                                                                    &#13;&#10;   tri[1] := ' Order by ' + tri[1];&#13;&#10;   &#13;&#10;   for i:=2 to 3 do&#13;&#10;   begin&#13;&#10;      if length(trim(tri[i]))&#62;0 then        &#13;&#10;         tri[i] := ' ,' + tri[i];&#13;&#10;   end;             &#13;&#10;       &#13;&#10;   tris:=tri[1] + tri[2]+tri[3];                                 &#13;&#10;                                             &#13;&#10;end;&#13;&#10;            &#13;&#10;    &#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                        &#13;&#10;         &#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);                              &#13;&#10;begin&#13;&#10;   FirstRecord('Categ');  &#13;&#10;   while not QueryEof('Categ') do                      //Remplissage du ComboBox &#34;Catégorie&#34;                                                                                   &#13;&#10;       begin                                                                                &#13;&#10;               CheckListBox1.Items.Add(&#60;Categ.&#34;code&#34;&#62;);&#13;&#10;               NextRecord('Categ');                                                           &#13;&#10;       end;&#13;&#10;&#13;&#10;   DialogPage1.Caption := Translate('IDS_HISTORIQUESTOCK','');&#13;&#10;&#13;&#10;   GroupBox1.Caption := Translate('IDS_CRITERES_SELECTION','');                //GroupBoxes                                                      &#13;&#10;   GroupBox2.Caption := Translate('IDS_CRITERES_TRI','');&#13;&#10;   GroupBox3.Caption := Translate('IDS_VALORISATION','');&#13;&#10;   GroupBox4.Caption := Translate('IDS_TYPE_MOUVEMENT','');       &#13;&#10;     &#13;&#10;   Label1.Caption := Translate('IDS_ARTICLE_DE','');                       //Labels                                                                   &#13;&#10;   Label2.Caption := Translate('IDS_JUSQUE','');&#13;&#10;   Label3.Caption := Translate('IDS_CATEGORIE','');&#13;&#10;   Label4.Caption := Translate('IDS_FOURNISSEUR_PRINCIPAL','');&#13;&#10;   Label5.Caption := Translate('IDS_DATES_DE','');&#13;&#10;   Label6.Caption := Translate('IDS_JUSQUE','');&#13;&#10;&#13;&#10;   CheckBox1.Caption := Translate('IDS_ARTICLE','');                      //CheckBoxes                                                                                                                          &#13;&#10;   CheckBox2.Caption := Translate('IDS_CATEGORIESARTICLES','');&#13;&#10;   CheckBox3.Caption := Translate('IDS_FOURNISSEUR_PRINCIPAL','');&#13;&#10;   CheckBox4.Caption := Translate('IDS_POSTE','');&#13;&#10;   CheckBox5.Caption := Translate('IDS_FOURNISSEUR','');&#13;&#10;   CheckBox6.Caption := Translate('DOC_CLIENT','');&#13;&#10;   CheckBox7.Caption := Translate('IDS_EMPLACEMENT','');&#13;&#10;   CheckBox8.Caption := Translate('IDS_ENTREES','');&#13;&#10;   CheckBox9.Caption := Translate('IDS_SORTIES','');&#13;&#10;   CheckBox10.Caption := Translate('IDS_CORRECTIONS','');&#13;&#10;   CheckBox11.Caption := Translate('IDS_TRANSFERTS','');&#13;&#10;&#13;&#10;   BitBtn2.Caption := Translate('IDS_CANCEL','');                                                                               &#13;&#10;                      &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                &#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                  &#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                  &#13;&#10;    &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);                                                &#13;&#10;var &#13;&#10;   sql1 : string;&#13;&#10;   compl : string;&#13;&#10;   strs : ARRAY[1..3] of string;                                                       &#13;&#10;   from : string;&#13;&#10;   i : Integer;&#13;&#10;   cat : string;                                       &#13;&#10;&#13;&#10;begin&#13;&#10;   sql1 := 'Select j.article, j.local, j.date, j.numdoc, j.type, j.qte, j.poste, j.fourn, a.descriptio, a.categorie from jnlstock j join article a on j.article=a.code';&#13;&#10;   &#13;&#10;     &#13;&#10;   //+++ Section Critères de sélection +++                                                                                        &#13;&#10;&#13;&#10;&#13;&#10;   if not (isblank(Edit1.Text)) then&#13;&#10;      strs[1] := ' a.code&#62;''' + Edit1.Text+'''';                                  &#13;&#10;   if not (isblank(Edit2.Text)) then&#13;&#10;      strs[2] := ' a.code&#60;''' + Edit2.Text+'''';                                  &#13;&#10;   if not (isblank(Edit3.Text)) then&#13;&#10;      strs[3] := ' a.codefou=''' + Edit3.Text + '''';                                  &#13;&#10;       &#13;&#10;   compl := '';       &#13;&#10;   &#13;&#10;   for i:=1 to 3 do&#13;&#10;   begin                                                         //Construction du &#34;Where&#34;                                                    &#13;&#10;      if isblank(compl) then&#13;&#10;         compl := strs[i]&#13;&#10;      else&#13;&#10;         if not (isblank(strs[i])) then                                     &#13;&#10;            compl := compl + ' and' +  strs[i];&#13;&#10;   end;      &#13;&#10;  &#13;&#10;  //conditions sur les dates&#13;&#10;   if not (isblank(compl)) then    &#13;&#10;      compl := compl + ' and j.date&#62;=:DateDebut and j.date&#60;=:DateFin'&#13;&#10;   else&#13;&#10;      compl := 'j.date&#62;=:DateDebut and j.date&#60;=:DateFin';&#13;&#10; &#13;&#10;                                                                                                                                                                                        &#13;&#10;   cat := ' and (';                                 &#13;&#10;&#13;&#10; // sélection des catégories                                                                                       &#13;&#10;   for i:=0 to CheckListBox1.Items.Count-1 do&#13;&#10;   begin&#13;&#10;      if CheckListBox1.Checked[i] then&#13;&#10;      begin                    &#13;&#10;         compl := compl + cat + 'a.categorie = ''' + CheckListBox1.Items[i] + '''';&#13;&#10;         cat := ' or ';                                                                             &#13;&#10;      end;                                    &#13;&#10;   end;                &#13;&#10;&#13;&#10;   compl  := compl + ')';                                              &#13;&#10;&#13;&#10; //  compl := compl + cat;                                                   &#13;&#10;     &#13;&#10;   if not (isblank(compl)) then&#13;&#10;   begin               &#13;&#10;      sql1 := sql1 + ' Where ' + compl;                              &#13;&#10;   end;        &#13;&#10;     &#13;&#10;      &#13;&#10;   OrdreTris;&#13;&#10;&#13;&#10;   sql1 := sql1 + tris;                                                     &#13;&#10;&#13;&#10;        &#13;&#10;   ReplaceSQL('JnlStk',sql1);                           &#13;&#10;   &#13;&#10;   ReplaceParam('JnlStk:DateDebut','ftDateTime',DateEdit1.Date);&#13;&#10;   ReplaceParam('JnlStk:DateFin','ftDateTime',DateEdit2.Date);&#13;&#10;     &#13;&#10;//+++++++  Messages de debogage  ++++++++                                                                                                                     &#13;&#10;   ShowMessage('Requête SQL1: '+ sql1);&#13;&#10;&#13;&#10;    &#13;&#10;end;&#13;&#10;    &#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure SpeedButton1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Edit1.Text := Browse(Edit1.Text,'Article');                                                                                 &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++  &#13;&#10;  &#13;&#10;procedure SpeedButton2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   Edit2.Text := Browse(Edit2.Text,'Article');  &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if not CheckBox4.Checked then&#13;&#10;   begin&#13;&#10;      Memo2.Visible := false;&#13;&#10;      JnlStknumdoc.Visible := false;                                                                      &#13;&#10;   end;&#13;&#10;  &#13;&#10;   if not CheckBox4.Checked then&#13;&#10;   begin&#13;&#10;      Memo3.Visible := false;&#13;&#10;      JnlStkfourn.Visible := false;          &#13;&#10;   end;&#13;&#10;  &#13;&#10;(*   if not CheckBox4.Checked then&#13;&#10;      Memo2.Visible := false;&#13;&#10;        &#13;&#10;     *)&#13;&#10;   if not CheckBox4.Checked then&#13;&#10;   begin&#13;&#10;      Memo4.Visible := false;&#13;&#10;      JnlStklocal.Visible := false;          &#13;&#10;   end;&#13;&#10;&#13;&#10;   Memo1.Text := Translate('IDS_HISTORIQUESTOCK','');                                                                                &#13;&#10;   Memo2.Text := Translate('IDS_POSTE','');                                                                                &#13;&#10;   Memo3.Text := Translate('IDS_FOURNISS','');                                                                                &#13;&#10;   Memo4.Text := Translate('IDS_MAGASIN','');                                                                                &#13;&#10;   Memo5.Text := Translate('DOC_DATE','');                                                                                &#13;&#10;   Memo6.Text := Translate('IDS_TYPE','');                                                                                &#13;&#10;   Memo7.Text := Translate('DOC_QUANTITE','');                                                                                &#13;&#10;   Memo8.Text := Translate('IDS_STOCK','');                                                                                &#13;&#10;                       &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure SpeedButton3OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   Edit3.Text := browse(Edit3.Text,'Fournisseur');                                                &#13;&#10;end;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;&#13;&#10;  &#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
