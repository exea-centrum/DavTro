gth(trim(tri[i]))&#62;0 then        &#13;&#10;         tri[i] := ' ,' + tri[i];&#13;&#10;   end;             &#13;&#10;       &#13;&#10;   tris:=tri[1] + tri[2]+tri[3];                                 &#13;&#10;     &#13;&#10;end;        &#13;&#10;&#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                &#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;  &#13;&#10;procedure SpeedButton1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Edit1.Text:=browse(Edit1.Text,'Fournisseur');                                          &#13;&#10;end;&#13;&#10;&#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++    &#13;&#10;  &#13;&#10;procedure SpeedButton2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   Edit2.Text:=browse(Edit2.Text,'Fournisseur');                                &#13;&#10;end;&#13;&#10;&#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++&#13;&#10;//+++++++++++++++++++++++++++++++++++++++++++++++++++++    &#13;&#10;&#13;&#10;&#13;&#10;//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++                                                                                                                                    &#13;&#10;   &#13;&#10;procedure ClientsconditionOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;str :string;                           &#13;&#10;begin&#13;&#10;   if not (isblank(&#60;Fourn.&#34;numcli&#34;&#62;)) then&#13;&#10;   begin&#13;&#10;      str := Translate('DOC_NOCLIENT','')+ ': ' + &#60;Fourn.&#34;numcli&#34;&#62;;                             &#13;&#10;      Clientscondition.Memo.Add(str);&#13;&#10;   end;&#13;&#10;   &#13;&#10;   if not (&#60;Fourn.&#34;delaipaiem&#34;&#62;=0) then&#13;&#10;   begin&#13;&#10;      str := Translate('DOC_PAIEMENT','')+ ': ';&#13;&#1