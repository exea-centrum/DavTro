#34;&#62;)) &#60;&#62; 0 then&#13;&#10;          begin              &#13;&#10;            nomsoc.visible := false;  &#13;&#10;            sstsoc.visible := false;  &#13;&#10;            txt1soc.visible := false;  &#13;&#10;            logosoc.visible := true;&#13;&#10;            logosoc.loadfromfile(&#60;societe.&#34;logo&#34;&#62;);&#13;&#10;            //logosoc.width := GetEMFWidth('logosoc');&#13;&#10;            //logosoc.height := getemfheight('logosoc');  &#13;&#10;            logosoc.width := &#60;settings.&#34;logow&#34;&#62; * valpouce * unitpix;&#13;&#10;            logosoc.height := &#60;settings.&#34;logow&#34;&#62; * valpouce * unitpix;  &#13;&#10;            logosoc.left := &#60;settings.&#34;logol&#34;&#62; * valpouce;&#13;&#10;            logosoc.top := &#60;settings.&#34;logot&#34;&#62; * valpouce;  &#13;&#10;            comfouhclient.left := logosoc.left;&#13;&#10;            comfouhclient.top := logosoc.top + logosoc.height;                                                                                                        &#13;&#10;            comfouhclient.width := logosoc.width;&#13;&#10;          end;&#13;&#10;     end             &#13;&#10;  else               &#13;&#10;    begin&#13;&#10;      // Préférences système&#13;&#10;      // Société&#13;&#10;      nomsoc.visible := true;  &#13;&#10;      sstsoc.visible := true;  &#13;&#10;      txt1soc.visible := true;  &#13;&#10;      logosoc.visible := false;&#13;&#10;      nomsoc.left := &#60;settings.&#34;compl&#34;&#62; * valpouce;&#13;&#10;      nomsoc.top := &#60;settings.&#34;compt&#34;&#62; * valpouce;  &#13;&#10;      nomsoc.width := &#60;settings.&#34;compw&#34;&#62; * valpouce * unitpix;&#13;&#10;      sstsoc.left := nomsoc.left;                                                                                                &#13;&#10;      sstsoc.width := nomsoc.width;&#13;&#10;      sstsoc.top := nomsoc.top + nomsoc.height;&#13;&#10;      txt1soc.left := nomsoc.left;                                                                                                &#13;&#10;      txt1soc.width := nomsoc.width;                                                                                                &#13;&#10;      txt1soc.top := sstsoc.top + sstsoc.height;&#13;&#10;      comfouhclient.left := nomsoc.left;&#13;&#10;      comfouhclient.top := txt1soc.top + txt1soc.height;&#13;&#10;      comfouhclient.width := nomsoc.width;&#13;&#10;    end            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure comfouhclientOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var      &#13;&#10;  chaine0 : string;&#13;&#10;&#13;&#10;begin&#13;&#10;// Préférences fournisseur                    &#13;&#10;// Numéro du client/fabricant chez le fournisseur                                                                                                                &#13;&#10;  if length(Trim(&#60;fourn.&#34;numcli&#34;&#62;)) &#60;&#62; 0 then                                &#13;&#10;     begin&#13;&#10;       chaine0 := Translate('DOC_CLIENT',&#60;fourn.&#34;langue&#34;&#62;);  &#13;&#10;       comfouhclient.memo.add(chaine0 + ' ' + &#60;fourn.&#34;numcli&#34;&#62;);&#13;&#10;     end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure NosRefOnBeforePrint(Sender: TfrxComponent);&#13;&#10;// Preferences utilisateur&#13;&#10;// Premiere colonne article ou description d'article                                                                                                                                                              &#13;&#10;begin&#13;&#10;  If &#60;settings.&#34;fircol&#34;&#62; = 1 then&#13;&#10;     nosref.memo.add(&#60;comfou.&#34;article&#34;&#62;)&#13;&#10;  else&#13;&#10;     nosref.memo.add(&#60;comfou.&#34;descriptio&#34;&#62;);                                     &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure Child2OnAfterPrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;largdispo : extended;&#13;&#10;                                            &#13;&#10;begin&#13;&#10;// Largeur totale tous champs compris                                                                      &#13;&#10;  largdispo := page1.paperwidth * unitpix - page1.leftmargin * unitpix - page1.rightmargin * unitpix - ET_NumLin.width - ET_QteRecue.width - ET_Prix.width - ET_Total.width - ET_Solde.width - ET_PrixDev.width - ET_TotalDev.width - ET_Rem.width;&#13;&#10;  &#13;&#10;// Preferences utilisateur&#13;&#10;// Numerotation  de ligne                                                         &#13;&#10;  if &#60;settings.&#34;linum&#34;&#62; = 0 then&#13;&#10;     begin&#13;&#10;       largdispo := largdispo + ET_NumLin.width;                                                                                               &#13;&#10;       ET_NosRef.left := 0;&#13;&#10;       ET_NumLin.visible := false;&#13;&#10;       NumLin.visible := false;                                  &#13;&#10;     end&#13;&#10;  else&#13;&#10;     begin&#13;&#10;       ET_NosRef.left := ET_NumLin.left + ET_NumLin.width;&#13;&#10;       ET_NumLin.visible := true;&#13;&#10;       NumLin.visible := true;                                  &#13;&#10;     end;&#13;&#10;                    &#13;&#10;  ET_NosRef.width := 1/3 * largdispo;           &#13;&#10;  ET_VosRef.width := 2/3 * largdispo;           &#13;&#10;  ET_QteRecue.left := ET_NosRef.left + ET_NosRef.width;&#13;&#10;  ET_VosRef.left := ET_QteRecue.left + ET_QteRecue.width;&#13;&#10;  ET_Prix.left := ET_VosRef.left + ET_VosRef.width;&#13;&#10;  ET_Total.left := ET_Prix.left + ET_Prix.width;&#13;&#10;  ET_Solde.left := ET_Total.left + ET_Total.width;&#13;&#10;  ET_PrixDev.left := ET_Solde.left + ET_Solde.width;&#13;&#10;  ET_TotalDev.left := ET_PrixDev.left + ET_PrixDev.width;&#13;&#10;  ET_Rem.left := ET_TotalDev.left + ET_TotalDev.width;&#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;      totcde.width := ET_TotalDev.width + ET_Rem.width;&#