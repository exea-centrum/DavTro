<TfrxReport Version="4.2.14" DotMatrixReport="false" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Par défaut" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="39226,6065628009" ReportOptions.Description.Text="" ReportOptions.Name="ETIQUETTES OPTIM" ReportOptions.LastChange="39233,7777770949" ScriptLanguage="PascalScript" ScriptText.Text="// -----                 &#13;&#10;// Variables générales                                                            &#13;&#10;// -----                 &#13;&#10;var&#13;&#10;sql0 : string;      &#13;&#10;unitpix : extended;&#13;&#10;valpouce : extended;&#13;&#10;LargeurDessinAngle1 : extended;                                &#13;&#10;OrigineComposant : string;&#13;&#10;a:string;                    &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;//var&#13;&#10;//Memo4 : TfrxMemoView;                                              &#13;&#10;begin&#13;&#10;TypeDB(a);        &#13;&#10;   //nombre de colonnes&#13;&#10;   MasterData1.Columns:=&#60;Settings.&#34;Colonne&#34;&#62;;&#13;&#10;&#13;&#10;   //largeur de l'étiquette                                                                              &#13;&#10;   MasterData1.ColumnWidth:=(&#60;Settings.&#34;Largeur&#34;&#62; + (&#60;Settings.&#34;EntreColonne&#34;&#62; / 2)) * unitpix ;&#13;&#10;&#13;&#10;   //hauteur de l'étiquette                                                       &#13;&#10;   MasterData1.Height:=(&#60;Settings.&#34;Hauteur&#34;&#62; + &#60;Settings.&#34;EntreLigne&#34;&#62;) * unitpix ;&#13;&#10;&#13;&#10;   // dessin du rectangle arrondi (= dimension étiquette)                                                                                              &#13;&#10;   Shape1.Width:=&#60;Settings.&#34;Largeur&#34;&#62; * unitpix;&#13;&#10;   Shape1.Height:=&#60;Settings.&#34;Hauteur&#34;&#62; * unitpix;&#13;&#10;           &#13;&#10;   // pour instruction PRENDRE                                                          &#13;&#10;   if (&#60;Detlot.&#34;instruct&#34;&#62;)='P' then&#13;&#10;      begin&#13;&#10;&#13;&#10;         // Numéro de l'étiquette&#13;&#10;         Detlotligne.visible:=True;                                           &#13;&#10;           &#13;&#10;         // Numéro de scie                                                                                    &#13;&#10;         Memo1.visible:=true;&#13;&#10;         Memo1.Width:=MasterData1.ColumnWidth;                                         &#13;&#10;         Memo1.Left:=0;        &#13;&#10;         Memo1.HAlign:=haCenter;                                                     &#13;&#10;         Memo1.top:=2 * (MasterData1.Height/3);                                     &#13;&#10;&#13;&#10;         // code du profil et sa couleur interieur ou (couleur interieur et couleur exterieur)&#13;&#10;         Memo2.Width:=MasterData1.ColumnWidth;                                         &#13;&#10;         Memo2.Left:=0;&#13;&#10;         Memo2.HAlign:=haCenter;                                                     &#13;&#10;         Memo2.Top:=(MasterData1.Height / 6);&#13;&#10;&#13;&#10;         // description du profil                                                                     &#13;&#10;         Profildescriptio.Visible:=true;&#13;&#10;         Profildescriptio.Width:=MasterData1.ColumnWidth;&#13;&#10;         Profildescriptio.Left:=0;&#13;&#10;         Profildescriptio.HAlign:=haCenter;&#13;&#10;         Profildescriptio.Top:=(MasterData1.Height / 3);&#13;&#10;&#13;&#10;         // instruction                                        &#13;&#10;         Memo3.Visible:=true;&#13;&#10;         Memo3.Width:=MasterData1.ColumnWidth;                                       &#13;&#10;         Memo3.Left:=0;&#13;&#10;         Memo3.HAlign:=haCenter;                                                         &#13;&#10;         Memo3.Top:=(MasterData1.Height / 2);&#13;&#10;&#13;&#10;         Line1.Visible:=False;&#13;&#10;         Memo4.visible:=False;&#13;&#10;&#13;&#10;         // Numéro de commande&#13;&#10;         Detlotnumero.visible:=False;                                                                 &#13;&#10;         // Nom du client&#13;&#10;         Clientnom.visible:=False;&#13;&#10;         // Référence de la commande                                                    &#13;&#10;         Detlotrefcli.visible:=False;                                                                 &#13;&#10;         // CodeBarre&#13;&#10;         BarCode1.visible:=False;&#13;&#10;         // Angle 1 et 2&#13;&#10;         Memo5.visible:=False;                                                                 &#13;&#10;         picture1.visible := False;&#13;&#10;         picture2.visible := False;&#13;&#10;         picture3.visible := False;&#13;&#10;         picture4.visible := False;&#13;&#10;         picture5.visible := False;&#13;&#10;         picture6.visible := False;       &#13;&#10;         picture7.visible := False;&#13;&#10;         picture8.visible := False;&#13;&#10;         picture9.visible := False;&#13;&#10;         picture10.visible := False;&#13;&#10;         picture11.visible := False;&#13;&#10;         picture12.visible := False;       &#13;&#10;         Memo7.Visible:=False;                                    &#13;&#10;         Memo8.Visible:=False;                                    &#13;&#10;         Memo9.Visible:=False;                                    &#13;&#10;         Line2.Visible:=False;                                                        &#13;&#10;                       &#13;&#10;      end&#13;&#10;&#13;&#10;   // pour instruction SOCKER, JETER ou CHUTE&#13;&#10;   // attention impression de l'etiquette JETER si &#60;Settings.&#34;Jeter&#34;&#62;=1                                                                                                                            &#13;&#10;   else if ((&#60;Detlot.&#34;instruct&#34;&#62;)='S') OR ((&#60;Detlot.&#34;instruct&#34;&#62;)='J') OR ((&#60;Detlot.&#34;instruct&#34;&#62;)='H') then&#13;&#10;      begin&#13;&#10;&#13;&#10;         // Numéro de l'étiquette&#13;&#10;         Detlotligne.visible:=True;                                           &#13;&#10;&#13;&#10;         // code du profil et sa couleur interieur ou (couleur interieur et couleur exterieur)&#13;&#10;         Memo2.Width:=MasterData1.ColumnWidth;                                         &#13;&#10;         Memo2.Left:=0;&#13;&#10;         Memo2.HAlign:=haCenter;                                                     &#13;&#10;         Memo2.Top:=(MasterData1.Height / 6);&#13;&#10;          &#13;&#10;         // description du profil                                                                     &#13;&#10;         Profildescriptio.Visible:=true;&#13;&#10;         Profildescriptio.Width:=MasterData1.ColumnWidth;&#13;&#10;         Profildescriptio.Left:=0;&#13;&#10;         Profildescriptio.HAlign:=haCenter;&#13;&#10;         Profildescriptio.Top:=(MasterData1.Height / 3);&#13;&#10;  &#13;&#10;         Line1.Visible:=False;&#13;&#10;         Memo4.visible:=False;&#13;&#10;         // Numéro de commande&#13;&#10;         Detlotnumero.visible:=False;&#13;&#10;         // Nom du client&#13;&#10;         Clientnom.visible:=False;             &#13;&#10;         // Référence de la commande                                                    &#13;&#10;         Detlotrefcli.visible:=False;                                                                 &#13;&#10;         // CodeBarre&#13;&#10;         BarCode1.visible:=True;&#13;&#10;         BarCode1.Width:=MasterData1.ColumnWidth ;                                         &#13;&#10;         BarCode1.Left:=0;&#13;&#10;         BarCode1.HAlign:=haCenter;                                                     &#13;&#10;         BarCode1.Top:=(MasterData1.Height / 1.5);&#13;&#10;         // Angle 1 et 2&#13;&#10;         Memo5.visible:=False;                                                                 &#13;&#10;         Memo6.visible:=False;                                                                 &#13;&#10;         picture1.visible := False;&#13;&#10;         picture2.visible := False;&#13;&#10;         picture3.visible := False;&#13;&#10;         picture4.visible := False;&#13;&#10;         picture5.visible := False;&#13;&#10;         picture6.visible := False;       &#13;&#10;         picture7.visible := False;&#13;&#10;         picture8.visible := False;&#13;&#10;         picture9.visible := False;&#13;&#10;         picture10.visible := False;&#13;&#10;         picture11.visible := False;&#13;&#10;         picture12.visible := False;       &#13;&#10;         Memo7.Visible:=False;                                    &#13;&#10;         Memo8.Visible:=False;                                    &#13;&#10;         Memo9.Visible:=False;                                    &#13;&#10;         Line2.Visible:=False;                                                        &#13;&#10;      end&#13;&#10;&#13;&#10;   // pour instruction COUPER                                                                                  &#13;&#10;   else if (&#60;Detlot.&#34;instruct&#34;&#62;)='C' then          &#13;&#10;      begin&#13;&#10;         // Numéro de l'étiquette&#13;&#10;         Detlotligne.visible:=False;                                           &#13;&#10;&#13;&#10;         // Numéro de scie&#13;&#10;         memo1.visible:=false;&#13;&#10;&#13;&#10;         // code du profil et sa couleur interieur ou (couleur interieur et couleur exterieur)&#13;&#10;         Memo2.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;         Memo2.Left:=0;&#13;&#10;         Memo2.HAlign:=haLeft;                                                     &#13;&#10;         Memo2.Top:=(MasterData1.Height / 20);&#13;&#10;&#13;&#10;         // description du profil                                                                     &#13;&#10;         Profildescriptio.Visible:=False;&#13;&#10;    &#13;&#10;         //Ligne Verticale pour separer le dessin                                                                                  &#13;&#10;         Line1.Visible:=True;&#13;&#10;         Line1.Height:=Shape1.Height;&#13;&#10;         Line1.Left:=Shape1.Width - (Shape1.Width / 3);&#13;&#10;&#13;&#10;         // Dimension à couper + lot&#13;&#10;         //Memo4:=TfrxMemoView.create(MasterData1);&#13;&#10;         Memo4.visible:=True;                                                                                    &#13;&#10;         Memo4.Text:= VarToStr(&#60;Detlot.&#34;qte&#34;&#62;) + ' x ' +  VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' - ' + Translate('IDS_CASIER','') + ' '  + VarToStr(&#60;Detlot.&#34;casier&#34;&#62;) + ' - ' + Translate('IDS_LOT','') + ' '  + VarToStr(&#60;Detlot.&#34;lot&#34;&#62;);&#13;&#10;         Memo4.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;         Memo4.Left:=0;&#13;&#10;         Memo4.HAlign:=haLeft;                                                     &#13;&#10;         Memo4.Top:=(MasterData1.Height / 6);&#13;&#10;&#13;&#10;         // Numéro de commande&#13;&#10;         Detlotnumero.visible:=True;                                                                 &#13;&#10;         Detlotnumero.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;         Detlotnumero.Left:=0;&#13;&#10;         Detlotnumero.HAlign:=haLeft;                                                     &#13;&#10;         Detlotnumero.Top:=(MasterData1.Height / 3.5);&#13;&#10;&#13;&#10;         // Nom du client&#13;&#10;         if (&#60;Settings.&#34;NomClient&#34;&#62;)=1 then&#13;&#10;         begin                       &#13;&#10;            Clientnom.visible:=True;                                                                 &#13;&#10;            Clientnom.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;            Clientnom.Left:=0;&#13;&#10;            Clientnom.HAlign:=haLeft;                                                     &#13;&#10;            Clientnom.Top:=(MasterData1.Height / 2.5);&#13;&#10;         end;                                 &#13;&#10;&#13;&#10;         // Référence de la commande                                                    &#13;&#10;         if (&#60;Settings.&#34;Reference&#34;&#62;)=1 then&#13;&#10;         begin                       &#13;&#10;            Detlotrefcli.visible:=True;                                                                 &#13;&#10;            Detlotrefcli.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;            Detlotrefcli.Left:=0;&#13;&#10;            Detlotrefcli.HAlign:=haLeft;                                                     &#13;&#10;            Detlotrefcli.Top:=(MasterData1.Height / 2);&#13;&#10;         end;  &#13;&#10;&#13;&#10;         // CodeBarre&#13;&#10;         BarCode1.visible:=True;&#13;&#10;         BarCode1.Width:=2 * (MasterData1.ColumnWidth / 3);                                         &#13;&#10;         BarCode1.Left:=0;&#13;&#10;         BarCode1.HAlign:=haLeft;                                                     &#13;&#10;         BarCode1.Top:=(MasterData1.Height /1.5);&#13;&#10;                                                                                                                             &#13;&#10;         // Angle 1 et 2&#13;&#10;         Memo5.visible:=True;                                                                 &#13;&#10;         Memo5.text := VarToStr(&#60;Detlot.&#34;Angle1&#34;&#62;) + '°';    &#13;&#10;         Memo5.Left:=10;&#13;&#10;         Memo5.Top:=(MasterData1.Height / 1.25);&#13;&#10;         LargeurDessinAngle1:=picture1.Width;                                                                                               &#13;&#10;         Memo6.visible:=True;&#13;&#10;         Memo6.text := VarToStr(&#60;Detlot.&#34;Angle2&#34;&#62;) + '°';    &#13;&#10;         Memo6.Left:=Memo5.Width + (2*LargeurDessinAngle1)+10;&#13;&#10;         Memo6.Top:=(MasterData1.Height / 1.25);&#13;&#10;           &#13;&#10;         // Dessin Angle 1 --------------------------------------------------------------------                                                                                                                                                                                                                &#13;&#10;         if (&#60;Detlot.&#34;angle1&#34;&#62;)=45 then&#13;&#10;            begin&#13;&#10;               picture1.visible:=true;&#13;&#10;               picture1.left:= 10 + Memo5.Width ;&#13;&#10;               picture1.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;               LargeurDessinAngle1:=picture1.Width;                                                                                               &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle1&#34;&#62;)=90 then                            &#13;&#10;            begin                     &#13;&#10;               picture2.visible:=true;&#13;&#10;               picture2.left:= 10 + Memo5.Width ;&#13;&#10;               picture2.top:=(MasterData1.Height / 1.25);&#13;&#10;               LargeurDessinAngle1:=picture2.Width;                                                                                               &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle1&#34;&#62;)=0 then&#13;&#10;            begin                     &#13;&#10;               picture3.visible:=true;&#13;&#10;               picture3.left:= 10 + Memo5.Width ;&#13;&#10;               picture3.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;               LargeurDessinAngle1:=picture3.Width;                                                                                               &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle1&#34;&#62;)=200 then                            &#13;&#10;            begin                     &#13;&#10;               picture4.visible:=true;&#13;&#10;               picture4.left:= 10 + Memo5.Width ;&#13;&#10;               picture4.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;               LargeurDessinAngle1:=picture4.Width;                                                                                               &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle1&#34;&#62;)=250 then&#13;&#10;            begin                     &#13;&#10;               picture5.visible:=true;&#13;&#10;               picture5.left:= 10 + Memo5.Width ;&#13;&#10;               picture5.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;               LargeurDessinAngle1:=picture5.Width;                                                                                               &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle1&#34;&#62;)=300 then&#13;&#10;            begin                     &#13;&#10;               picture6.visible:=true;&#13;&#10;               picture6.left:= 10 + Memo5.Width ;&#13;&#10;               picture6.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;               LargeurDessinAngle1:=picture6.Width;                                                                                               &#13;&#10;            end;                       &#13;&#10;         // Fin Dessin Angle 1 _____________________________________________________________                                                                                                                                                                                            &#13;&#10;&#13;&#10;         // Dessin Angle 2 ------------------------------------------------------------------                                                                &#13;&#10;         if (&#60;Detlot.&#34;angle2&#34;&#62;)=45 then&#13;&#10;            begin&#13;&#10;               picture7.visible:=true;&#13;&#10;               picture7.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5;&#13;&#10;               picture7.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=90 then                            &#13;&#10;            begin                     &#13;&#10;               picture8.visible:=true;&#13;&#10;               picture8.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5;&#13;&#10;               picture8.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=0 then&#13;&#10;            begin                     &#13;&#10;               picture9.visible:=true;&#13;&#10;               picture9.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5 ;&#13;&#10;               picture9.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=200 then                            &#13;&#10;            begin                     &#13;&#10;               picture10.visible:=true;&#13;&#10;               picture10.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5 ;&#13;&#10;               picture10.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=250 then&#13;&#10;            begin                     &#13;&#10;               picture11.visible:=true;&#13;&#10;               picture11.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5;&#13;&#10;               picture11.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=300 then&#13;&#10;            begin                     &#13;&#10;               picture12.visible:=true;&#13;&#10;               picture12.left:= 10 + Memo5.Width + LargeurDessinAngle1 - 5 ;&#13;&#10;               picture12.top:=(MasterData1.Height / 1.25);                                                 &#13;&#10;            end;                     &#13;&#10;         // Fin Dessin Angle 2 _____________________________________________________________                                                                                                                                                                                            &#13;&#10;&#13;&#10;         // Origine du composant: 0=inconnu; 1=dormant; 2=ouvrant; 3=traverse dormant; 4=traverse ouvrant; 5=profil périphérique; 6=coulisse; 7=tablier; 8=caisson; 9=croisillon; 14=parclose&#13;&#10;         // Position du composant: 1=gauche; 2=droite; 3=haut; 4=bas: 5=vertical; 6=horizontal; 7=haut gauche; 8=haut droite &#13;&#10;         case &#60;Detlot.&#34;origine&#34;&#62; of&#13;&#10;            '1' : OrigineComposant:='d';  &#13;&#10;            '2' : OrigineComposant:='o';  &#13;&#10;            '3' : OrigineComposant:='d';  &#13;&#10;            '4' : OrigineComposant:='d';  &#13;&#10;            '5' : OrigineComposant:='d';  &#13;&#10;            '6' : OrigineComposant:='d';  &#13;&#10;            '7' : OrigineComposant:='d';  &#13;&#10;            '8' : OrigineComposant:='d';  &#13;&#10;            '9' : OrigineComposant:='d';  &#13;&#10;            '14' : OrigineComposant:='d';  &#13;&#10;         end;&#13;&#10;         case &#60;Detlot.&#34;position&#34;&#62; of&#13;&#10;            '1' : Memo7.Text:=OrigineComposant + 'G';  &#13;&#10;            '2' : Memo7.Text:=OrigineComposant + 'D';  &#13;&#10;            '3' : Memo7.Text:=OrigineComposant + 'H';  &#13;&#10;            '4' : Memo7.Text:=OrigineComposant + 'B';  &#13;&#10;            '5' : Memo7.Text:=OrigineComposant + 'V';  &#13;&#10;            '6' : Memo7.Text:=OrigineComposant + 'H';  &#13;&#10;            '7' : Memo7.Text:=OrigineComposant + 'HG';  &#13;&#10;            '8' : Memo7.Text:=OrigineComposant + 'HD';  &#13;&#10;         end;  &#13;&#10;         Memo7.visible:=True;&#13;&#10;         Memo7.Left:=Memo5.Width + (2*LargeurDessinAngle1)+ 10 + Memo6.Width;&#13;&#10;         Memo7.Top:=(MasterData1.Height / 1.3);&#13;&#10;&#13;&#10;         // Dimension du profil et rayon si &#60;&#62;0&#13;&#10;         Memo8.visible:=True;&#13;&#10;         Memo8.Width:=2* LargeurDessinAngle1;                                         &#13;&#10;         Memo8.Left:= 10 + Memo5.Width ;&#13;&#10;         Memo8.HAlign:=haCenter;                                                     &#13;&#10;         Memo8.Top:=(MasterData1.Height / 1.25);&#13;&#10;         if (&#60;Detlot.&#34;rayon&#34;&#62;)=0 then&#13;&#10;            Memo8.Text:= VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) &#13;&#10;         else&#13;&#10;            Memo8.Text:= VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' - ' + 'R=' + VarToStr(&#60;Detlot.&#34;rayon&#34;&#62;);&#13;&#10;             &#13;&#10;         // Dimension du cadre&#13;&#10;         if &#60;Settings.&#34;DimCadre&#34;&#62; = 1 then&#13;&#10;            begin&#13;&#10;               Memo9.visible:=True;&#13;&#10;               Memo9.Text:=VarToStr(&#60;Detlot.&#34;lcadre&#34;&#62;) + ' x ' + VarToStr(&#60;Detlot.&#34;hcadre&#34;&#62;);                                              &#13;&#10;               Memo9.Width:=2* LargeurDessinAngle1;                                         &#13;&#10;               Memo9.Left:= 2 * (Shape1.Width/3);&#13;&#10;               Memo9.Top:=(MasterData1.Height / 20);&#13;&#10;         //Ligne Horizontale sous dimension du cadre                                                                                                                                          &#13;&#10;               Line2.Visible:=True;&#13;&#10;               Line2.Top:=(Shape1.Height / 5);&#13;&#10;               Line2.Left:=2 * (Shape1.Width/3);                                       &#13;&#10;               Line2.Width:=Shape1.Width/3;&#13;&#10;            end                                                                         &#13;&#10;      end                         &#13;&#10;               &#13;&#10;end;&#13;&#10;&#13;&#10;// Numero de scie                                       &#13;&#10;procedure Memo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   Memo1.Text := Translate('IDS_SCIE','') + ' : ' + VarToStr(&#60;Detlot.&#34;scie&#34;&#62;);&#13;&#10;end;&#13;&#10;  &#13;&#10;   &#13;&#10;// code du profil et sa couleur interieur ou (couleur interieure + couleur exterieure)                                                                                                                                                                                             &#13;&#10;procedure Memo2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if (&#60;Detlot.&#34;teinteext&#34;&#62;)=(&#60;Detlot.&#34;teinteint&#34;&#62;) then&#13;&#10;      Memo2.Text := &#60;Detlot.&#34;profil&#34;&#62; + ' - ' + &#60;Detlot.&#34;teinteint&#34;&#62;&#13;&#10;   else&#13;&#10;      Memo2.Text := &#60;Detlot.&#34;profil&#34;&#62; + ' - ' + &#60;Detlot.&#34;teinteext&#34;&#62; + ' Ext / '+ &#60;Detlot.&#34;teinteint&#34;&#62; + ' Int';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if &#60;Detlot.&#34;instruct&#34;&#62;='S' then&#13;&#10;      Memo3.text := Translate('IDS_STOCKER','') + ' ' + VarToStr(&#60;Detlot.&#34;qte&#34;&#62;) + ' x ' + VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' mm'                                                           &#13;&#10;   else if &#60;Detlot.&#34;instruct&#34;&#62;='J' then&#13;&#10;      Memo3.text := Translate('IDS_JETER','') + ' ' + VarToStr(&#60;Detlot.&#34;qte&#34;&#62;) + ' x ' + VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' mm'  &#13;&#10;   else if &#60;Detlot.&#34;instruct&#34;&#62;='H' then&#13;&#10;      Memo3.text := Translate('IDS_CHUTE','') + ' ' + VarToStr(&#60;Detlot.&#34;qte&#34;&#62;) + ' x ' + VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' mm'                                                          &#13;&#10;   else if &#60;Detlot.&#34;instruct&#34;&#62;='P' then&#13;&#10;      Memo3.text := Translate('IDS_PRENDRE','') + ' ' + VarToStr(&#60;Detlot.&#34;qte&#34;&#62;) + ' x ' + VarToStr(&#60;Detlot.&#34;dim&#34;&#62;) + ' mm';                                                             &#13;&#10;&#13;&#10;   if &#60;Detlot.&#34;instruct&#34;&#62;='C' then&#13;&#10;      memo3.visible:=false          &#13;&#10;   else                     &#13;&#10;      memo3.visible:=true&#13;&#10; end;&#13;&#10;&#13;&#10;&#13;&#10;  &#13;&#10;&#13;&#10;// --------------------------------------------------------------------                                                                                                                              &#13;&#10;begin&#13;&#10;// Begin principal&#13;&#10;// -----                 &#13;&#10;  &#13;&#10;// Valeur du pixel en mm pour conversion des largeurs de page et des marges (en mm)&#13;&#10;// Les longueurs de champs width/left renvoient des pixels                                                                                                                             &#13;&#10;   unitpix := 3.78;&#13;&#10;     &#13;&#10;// Valeur du pouce en mm pour conversion                                                                                                                              &#13;&#10;   valpouce := 25.423;&#13;&#10;&#13;&#10;   // Marge haute                                   &#13;&#10;   Page1.TopMargin:=&#60;Settings.&#34;MargeSup&#34;&#62;;                                            &#13;&#10;&#13;&#10;   // Marge droite                                                  &#13;&#10;   Page1.RightMargin:=&#60;Settings.&#34;MargeDroite&#34;&#62;;&#13;&#10;&#13;&#10;   // Largeur de la page = Marge Gauche + (nombre d'étiquettes * par largeur d'étiquette) + intervalle entre colonnes + marge droite                                                                                                                                                                                                     &#13;&#10;   // Page1.PaperWidth:=Page1.LeftMargin + (&#60;Settings.&#34;Colonne&#34;&#62; * &#60;Settings.&#34;Largeur&#34;&#62;) + &#60;Settings.&#34;EntreColonne&#34;&#62; + &#60;Settings.&#34;MargeDroite&#34;&#62;;                                               &#13;&#10;   // donc la marge gauche :&#13;&#10;&#13;&#10;    Page1.LeftMargin:=Page1.PaperWidth - (&#60;Settings.&#34;Colonne&#34;&#62; * &#60;Settings.&#34;Largeur&#34;&#62;) - &#60;Settings.&#34;EntreColonne&#34;&#62; - &#60;Settings.&#34;MargeDroite&#34;&#62;;                                               &#13;&#10;    &#13;&#10;//   Page1.LeftMargin:=Page1.PaperWidth - (2* &#60;Settings.&#34;Largeur&#34;&#62;) - &#60;Settings.&#34;MargeDroite&#34;&#62;;&#13;&#10;                                                        &#13;&#10;end." PropData="">
