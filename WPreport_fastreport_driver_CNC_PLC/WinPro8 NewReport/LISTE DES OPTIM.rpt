;          BEGIN&#13;&#10;//      GroupHeader2.visible:=true;&#13;&#10;         Shape2.visible:=true;&#13;&#10;         Shape2.top:=4;&#13;&#10;         Shape2.Height:=15;                                                &#13;&#10;         Shape2.left:=(&#60;Detlot.&#34;dim&#34;&#62;/10);&#13;&#10;//         Shape2.width:=(&#60;Detlot.&#34;dim&#34;&#62;)/10;&#13;&#10;         Shape2.color:=clRed;&#13;&#10;  &#13;&#10;          END                  &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure Memo6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (&#60;Detlot.&#34;position&#34;&#62;)=1 then&#13;&#10;    Memo6.text:='Gauche'  &#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=2 then&#13;&#10;     Memo6.text:='Droite'                         &#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=3 then&#13;&#10;     Memo6.text:='Haut'&#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=4 then&#13;&#10;    Memo6.text:='Bas'&#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=5 then&#13;&#10;    Memo6.text:='Vertical'&#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=6 then&#13;&#10;    Memo6.text:='Horizontal'&#13;&#10;  else if (&#60;Detlot.&#34;position&#34;&#62;)=7 then&#13;&#10;    Memo6.text:='Haut Gauche'&#13;&#10;  else if(&#60;Detlot.&#34;position&#34;&#62;)=8 then&#13;&#10;   Memo6.text:='Haut Droite'    &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;Compteur : integer;&#13;&#10;        &#13;&#10;begin                                          &#13;&#10;   memo9.visible:=false;&#13;&#10;   masterdata1.height:=20;                                                     &#13;&#10;    &#13;&#10;// ----- pour ligne Couper -----------&#13;&#10;   IF (&#60;Detlot.&#34;instruct&#34;&#62;)='C' then&#13;&#10;      begin                &#13;&#10;         MasterData1.visible:=true;&#13;&#10;         Compteur:=Compteur+1;&#13;&#10;         memo10.text:=intToStr(Compteur);&#13;&#10;         memo11.visible:=true;&#13;&#10;         Detlotcasier.visible:=true;&#13;&#10;         memo9.visible:=false;&#13;&#10;         Detlotqte.visible:=true;&#13;&#10;         Detlotdim.visible:=true;&#13;&#10;         Detlotqte.font.color:=clBlack;&#13;&#10;         Detlotdim.font.color:=clBlack;                    &#13;&#10;      end&#13;&#10;&#13;&#10;// ----- pour ligne Chute ---------------                                                                                                 &#13;&#10;   Else if ((&#60;Detlot.&#34;instruct&#34;&#62;)='S') or ((&#60;Detlot.&#34;instruct&#34;&#62;)='J') or ((&#60;Detlot.&#34;instruct&#34;&#62;)='H') then&#13;&#10;      begin                                    &#13;&#10;         MasterData1.visible:=true;&#13;&#10;         Compteur:=Compteur+1;&#13;&#10;         memo10.text:=intToStr(Compteur);&#13;&#10;         memo9.visible:=true;&#13;&#10;         memo9.font.color:=clGreen;                                                              &#13;&#10;         memo11.visible:=false;&#13;&#10;         Detlotqte.visible:=false;&#13;&#10;         Detlotdim.visible:=false;&#13;&#10;         Detlotcasier.visible:=false;&#13;&#10;      end&#13;&#10;&#13;&#10;// -----pour ligne Prendre-----------&#13;&#10;   Else&#13;&#10;//IF (&#60;Detlot.&#34;instruct&#34;&#62;)='P' then&#13;&#10;      begin&#13;&#10;         MasterData1.visible:=false ;&#13;&#10;         Compteur:=0;&#13;&#10;        end;&#13;&#10;&#13;&#10;   picture1.visible := False;&#13;&#10;   picture2.visible := False;&#13;&#10;   picture3.visible := False;&#13;&#10;   picture4.visible := False;&#13;&#10;   picture5.visible := False;&#13;&#10;   picture6.visible := False;       &#13;&#10;   picture7.visible := False;&#13;&#10;   picture8.visible := False;&#13;&#10;   picture9.visible := False;&#13;&#10;   picture10.visible := False;&#13;&#10;   picture11.visible := False;&#13;&#10;   picture12.visible := False;       &#13;&#10;    &#13;&#10;//---- Affichage du rayon ----------------------------                                  &#13;&#10;   if (&#60;Detlot.&#34;rayon&#34;&#62;)=0 then&#13;&#10;      Detlotrayon.visible := False       &#13;&#10;   else                     &#13;&#10;      Detlotrayon.visible := True;      &#13;&#10;        &#13;&#10;// °°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°°                                                                                                                                                                           &#13;&#10;//------ Affichage angle de coupe à droite                                                                                                            &#13;&#10;   Detlotangle2.visible:=false;                                                                         &#13;&#10;   if ((&#60;Detlot.&#34;rayon&#34;&#62;)=0) and ((&#60;Detlot.&#34;instruct&#34;&#62;)='C') then&#13;&#10;      begin               &#13;&#10;         if (&#60;Detlot.&#34;angle2&#34;&#62;)=45 then&#13;&#10;            begin&#13;&#10;               picture7.visible:=true;&#13;&#10;               picture7.top:=3;                                                   &#13;&#10;               picture7.left:= 220;&#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=90 then                            &#13;&#10;            begin                     &#13;&#10;               picture8.visible:=true;&#13;&#10;               picture8.top:=3;                                                   &#13;&#10;               picture8.left:= 220;&#13;&#10;            end                   &#13;&#10;         else if (&#60;Detlot.&#34;angle2&#34;&#62;)=0 then&#13;&#10;            begin                     &#13;&#10;               picture9.visible:=true;&#13;&#10;               picture9.top:=3;                                                   &#13;&#10;               picture9.left:= 220;&#13;&#10;            end                  