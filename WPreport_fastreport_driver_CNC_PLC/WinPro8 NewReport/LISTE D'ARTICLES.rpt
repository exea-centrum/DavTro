a.prtarif, af.livraison, af.delai, af.cdemin, af.pafb, af.frais, af.pr, af.q1, af.numero';      &#13;&#10;     &#13;&#10;   //+++ Section Critères de sélection +++                                                                                        &#13;&#10;   if isblank(Edit1.Text) then&#13;&#10;   begin&#13;&#10;      if isblank(Edit3.Text) then&#13;&#10;      begin&#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= ''                                                                                                                                                                &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= ' a.codefou=''' + Edit4.Text + ''''          &#13;&#10;            end;&#13;&#10;         end&#13;&#10;         else&#13;&#10;         begin&#13;&#10;            compl:= ' a.categorie=''' + ComboBox1.Text + '''';  &#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;                &#13;&#10;         end;&#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         compl:= ' a.code&#60;=''' + Edit3.Text+'''';                                                                                                            &#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text +''''        &#13;&#10;            end;&#13;&#10;         end&#13;&#10;         else&#13;&#10;         begin&#13;&#10;            compl:= compl + ' and a.categorie=''' +  ComboBox1.Text+'''';  &#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;                &#13;&#10;         end;          &#13;&#10;      end;                &#13;&#10;   end&#13;&#10;   else&#13;&#10;   begin&#13;&#10;      compl := ' a.code&#62;=''' + Edit1.Text+'''';                                                                                         &#13;&#10;      if isblank(Edit3.Text) then&#13;&#10;      begin&#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                        &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= ' a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;&#13;&#10;         end&#13;&#10;         else&#13;&#10;         begin&#13;&#10;            compl:= ' a.categorie='''+  ComboBox1.Text+'''';  &#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                                                                                                                           &#13;&#10;            end&#13;&#10;            else                                                                                                                                                                     &#13;&#10;            begin&#13;&#10;               compl:= compl + ' and a.codefou=''' + Edit4.Text+''''        &#13;&#10;            end;                &#13;&#10;         end;&#13;&#10;      end&#13;&#10;      else&#13;&#10;      begin&#13;&#10;         compl:= compl + ' and a.code &#60;= ''' + Edit3.Text+'''';                                                                                                            &#13;&#10;         if isblank(ComboBox1.Text) then&#13;&#10;         begin&#13;&#10;            if isblank(Edit4.Text) then&#13;&#10;            begin&#13;&#10;               compl:= compl                                                                       