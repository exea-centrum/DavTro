[C]name=usglot
[M]params=284
select u.lot, u.code, u.position, u.position2, u.position3, d.codebarre, d.profil, case u.position4 when 1 then 'G' when 2 then 'D' end as sens
from usglot u 
inner join detlot d on u.lot=d.lot and u.ligne=d.ligne
where left(u.code, 4) = 'PFD-' and u.lot = :order
order by u.ligne
